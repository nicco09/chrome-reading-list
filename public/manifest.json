{
  "manifest_version": 2,
  "name": "Reading List",
  "author": "Nick Wanstrath",
  "version": "1.0.1",
  "description": "Manage your reading list",
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "48.png",
    "default_title": "Reading List",
    "default_popup": "popup.html"
  },
  "permissions": [
    "bookmarks",
    "tabs",
    "activeTab"
  ]
}
