{
  "manifest_version": 2,
  "name": "Reading List",
  "author": "Nick Wanstrath",
  "version": "1.0.1",
  "description": "Manage your reading list",
  "browser_action": {
    "default_icon": {
      "16": "assets/16.png",
      "32": "assets/16.png",
      "48": "assets/48.png",
      "128": "assets/128.png"
    },
    "default_title": "Click to save bookmark",
    "default_popup": "pages/popup.html"
  },
  "icons": {
    "16": "assets/16.png",
    "32": "assets/16.png",
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "background": {
    "page": "pages/background.html"
  },
  "chrome_url_overrides": {
    "newtab": "pages/tab.html"
  },
  "permissions": ["bookmarks", "tabs", "activeTab"],
  "minimum_chrome_version": "60",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
